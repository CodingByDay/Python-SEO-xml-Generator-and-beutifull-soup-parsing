<!doctype html>
<html lang="en">

<head>
    <title>Emmares</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <title>@ViewBag.Title</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/gatitaa/main.css" rel="stylesheet" />
    <link href="~/lib/gatitaa/custom.css" rel="stylesheet" />
    <link href="~/lib/gatitaa/custom-media.css" rel="stylesheet" />
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700|Montserrat:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
</head>
<body>
    <div id="wrapper">
        <section class="hero-section">
            <div class="row flex-row margin-l-r-0">
                <div class="col-sm-6 leftside padding-l-r-0">
                    <div class="child">
                        <div class="logo">
                            <img src="~/images/logo_2.svg" alt="logo">
                        </div>
                        <h1 class="hidden-xs">
                            Advanced<br>
                            email<br />
                            marketing.
                        </h1>
                        <div class="bottom-menu">
                            <div class="clearfix hidden-xs">
                                <div class="col-sm-6 padding-l-r-0">
                                    <ul class="list-unstyled menulist">
                                        <li><a href="javascript:void(0);">Home</a></li>
                                        <li><a href="javascript:void(0);">Plans</a></li>
                                        <li><a href="javascript:void(0);">Blog</a></li>
                                        <li><a href="javascript:void(0);">Contact</a></li>
                                    </ul>
                                </div>
                                <div class="col-sm-6 padding-l-r-0">
                                    <ul class="list-unstyled menulist">
                                        <li><a href="javascript:void(0);">About us</a></li>
                                        <li><a href="javascript:void(0);">Jobs</a></li>
                                        <li><a href="javascript:void(0);">Terms</a></li>
                                        <li><a href="javascript:void(0);">Privacy</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div>
                                <ul class="list-inline iconlist hidden-xs">
                                    <li><a href="javascript:void(0);"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="javascript:void(0);"><i class="fab fa-linkedin-in"></i></a></li>
                                    <li><a href="javascript:void(0);"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="javascript:void(0);"><i class="fab fa-medium-m"></i></a></li>
                                    <li><a href="javascript:void(0);"><i class="fab fa-telegram-plane"></i></a></li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-sm-6 rightside padding-l-r-0">
                    <div class="child">
                        <div class="register-loginbox-wrapper">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/common.js"></script>
    <script src="~/js/metisMenu.js"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>
